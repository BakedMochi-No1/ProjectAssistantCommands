  "Description":"Disable Verify adb",
  "Action":{
    "Command1":"settings put global development_settings_enabled 1",
	"Sleep1":"1",
	"Command2":"settings put global adb_enabled 1",
	"Sleep2":"1",
	"Command3":"settings put global verifier_verify_adb_installs 0",
	"Sleep3":"1",
	"Command4":"settings put global stay_on_while_plugged_in 3",
	"Sleep4":"1",
	"Command5":"getprop ro.vendor.serialno | xxd -r -p > /tmp/mac.bin",
	"Sleep5":"1",
	"Command6":"rtk_burnkey_tool w mac-sn /tmp/mac.bin",
	"Sleep6":"1",
	"Command7":"pm compile -m speed-profile -f -a",
	"Sleep7":"1"
  }
